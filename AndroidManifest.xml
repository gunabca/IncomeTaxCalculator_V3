<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.incometaxcalculator.cit"
    android:versionCode="3"
    android:versionName="3.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.incometaxcalculator.cit.Incometax"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
            <activity
            android:name=".Tax_Main"
           >
            <intent-filter>
                <action android:name="com.incometaxcalculator.cit.TAX_MAIN" />
  <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
              <activity
            android:name=".Deductions"
           >
            <intent-filter>
                <action android:name="com.incometaxcalculator.cit.DEDUCTIONS" />
  <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
            
              
              <activity
            android:name=".OnlyTax"
           >
            <intent-filter>
                <action android:name="com.incometaxcalculator.cit.ONLYTAX" />
  <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
